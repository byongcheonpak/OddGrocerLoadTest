# Base URL for testing
BASE_URL=https://m.oddgrocer.com

# Test Configuration
TEST_DURATION=300
MAX_VIRTUAL_USERS=100
RAMP_UP_TIME=60

# Load Test Profiles
LIGHT_LOAD_VU=5
MEDIUM_LOAD_VU=20
HEAVY_LOAD_VU=50
SPIKE_LOAD_VU=100

# Browser Test Settings
BROWSER_TIMEOUT=30000
VIEWPORT_WIDTH=375
VIEWPORT_HEIGHT=667

# Reporting
REPORT_FORMAT=html
ENABLE_METRICS=true

# Environment
NODE_ENV=test
LOG_LEVEL=info

# API Endpoints
MAIN_PAGE_ENDPOINT=/main?id=2
STORE_PAGE_ENDPOINT=/store/175

# Rate Limiting
REQUEST_TIMEOUT=10000
MAX_REDIRECTS=5